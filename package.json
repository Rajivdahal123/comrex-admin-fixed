{
  "name": "next-js-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "start": "env-cmd -f .env.production next dev",
    "start:dev": "env-cmd -f .env.development next dev",
    "start:staging": "env-cmd -f .env.staging next dev",
    "build:dev": "env-cmd .env.development next build",
    "build:staging": "env-cmd .env.staging next build",
    "build": "env-cmd .env.production next build",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "build-types": "tsc --noEmit --pretty",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.3",
    "axios": "^0.19.0",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "formik": "2.1.4",
    "http-proxy-middleware": "^1.0.4",
    "lodash": "^4.17.21",
    "next": "^11.1.0",
    "next-seo": "^4.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-toastify": "^7.0.4",
    "styled-jsx-plugin-postcss": "^4.0.1",
    "webpack": "^5.53.0",
    "yup": "0.29.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/yup": "^0.29.6",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.4"
  },
  "license": "ISC"
}
